{
  "name": "r-ide",
  "displayName": "R-IDE",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:r-ide.create-file-from-template"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ride-sidebar-view",
          "title": "R-IDE",
          "icon": "src/media/hubot.svg"
        }
      ]
    },
    "views": {
      "ride-sidebar-view": [
        {
          "type": "webview",
          "id": "sidebar-wizards",
          "name": "Wizards",
          "contextualTitle": "Wizards",
          "visibility": "collapsed",
          "initialSize": 3
        },
        {
          "type": "webview",
          "id": "sidebar-bags",
          "name": "ROS Bags",
          "contextualTitle": "ROS Bags",
          "visibility": "collapsed",
          "initialSize": 1
        },
        {
          "type": "webview",
          "id": "sidebar-visuals",
          "name": "Visualization Tools",
          "contextualTitle": "Visualization Tools",
          "visibility": "collapsed",
          "initialSize": 1
        },
        {
          "type": "webview",
          "id": "sidebar-topics",
          "name": "ROS Topic Tools",
          "contextualTitle": "ROS Topic Tools",
          "visibility": "collapsed",
          "initialSize": 1
        }
      ]
    },
    "commands": [
      {
        "command": "r-ide.create-msg",
        "title": "R-IDE: Create Message File",
        "arguments": [
          {
            "name": "path",
            "description": "Where would you like the message file to be placed?",
            "type": "uri"
          }
        ]
      },
      {
        "command": "r-ide.create-srv",
        "title": "R-IDE: Create Service File",
        "arguments": [
          {
            "name": "path",
            "description": "Where would you like the service file to be placed?",
            "type": "uri"
          }
        ]
      },
      {
        "command": "r-ide.create-package",
        "title": "R-IDE: Create a new ROS package",
        "arguments": [
          {
            "name": "package_name",
            "description": "What should the package be named?",
            "type": "text"
          },
          {
            "name": "directory",
            "description": "Where should the package be placed",
            "type": "workspaceFolder"
          }
        ]
      },
      {
        "command": "r-ide.register-package",
        "title": "R-IDE: Register a package",
        "arguments": [
          {
            "name": "packageDirectory",
            "description": "Select the package direcotry",
            "type": "workspaceFolder"
          }
        ]
      },
      {
        "command": "r-ide.register-msg",
        "title": "R-IDE: Register a message file with a package",
        "arguments": [
          {
            "name": "package",
            "description": "Select the package to register",
            "type": "workspaceFolder"
          },
          {
            "name": "msg",
            "description": "Select the message files to add",
            "type": "uri"
          }
        ]
      },
      {
        "command": "r-ide.register-srv",
        "title": "R-IDE: Register a service file with a package",
        "arguments": [
          {
            "name": "package",
            "description": "Select the package to register",
            "type": "workspaceFolder"
          },
          {
            "name": "srv",
            "description": "Select the service files to add",
            "type": "uri"
          }
        ]
      },
      {
        "command": "r-ide.open-topic-monitor",
        "title": "R-IDE: Open Topic Monitor"
      },
      {
        "command": "r-ide.update-package-list",
        "title": "R-IDE: Update the package list from rospack"
      }
    ],
    "snippets": [
      {
        "language": "python", 
        "path": "./src/snippets/snippets.json"
      },
      {
        "language": "C++", 
        "path": "./src/snippets/cpp-snippets.json"
      },
      {
        "language": "souce.ros.msg", 
        "path": "./src/snippets/srv-snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "rollup": "rollup -c -w"
  },
  "devDependencies": {
    "@bulatdashiev/svelte-slider": "^1.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.12",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vscode/test-electron": "^2.2.3",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "rollup": "^2.79.1",
    "rollup-plugin-svelte": "^7.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-preprocess": "^5.0.1",
    "typescript": "^4.9.5"
  },
  "extensionDependencies": [
    "ms-iot.vscode-ros"
  ],
  "dependencies": {
    "mongoose": "^6.9.2",
    "roslib": "^1.3.0",
    "slider": "^1.0.4"
  }
}
